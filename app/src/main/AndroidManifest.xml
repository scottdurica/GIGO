<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gigo.emroxriprap.com.gigo" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SummaryScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ItemsScreen"
            android:label="@string/title_activity_items_screen"
            android:parentActivityName=".SummaryScreen" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="gigo.emroxriprap.com.gigo.SummaryScreen" />
        </activity>
    </application>

</manifest>
